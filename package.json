{
  "name": "nuxt-dropzone-filelist",
  "version": "1.0.5",
  "description": "File list style for dropzone as Nuxt 3 module",
  "repository": "https://github.com/BananaAcid/nuxt-dropzone-filelist",
  "homepage": "https://codesandbox.io/p/sandbox/dropzone-tests-and-styles-r7rows",
  "license": "ISC",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "keywords": [
    "nuxt",
    "nuxt3",
    "nuxt.js",
    "vue3",
    "dropzone",
    "dropzonejs",
    "upload",
    "filelist"
  ],
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "^3.5.1",
    "dropzone": "^6.0.0-beta.2",
    "less": "^4.1.3",
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/module-builder": "^0.3.1",
    "@nuxt/schema": "^3.5.1",
    "@nuxt/test-utils": "^3.5.1",
    "@types/dropzone": "^5.7.4",
    "@types/node": "^20",
    "changelogen": "^0.5.3",
    "eslint": "^8.41.0",
    "npm-check-updates": "^16.10.12",
    "nuxt": "^3.5.1",
    "vitest": "^0.31.1"
  }
}